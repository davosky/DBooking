<div class='row'>
  <div class='col col-lg-12 col-md-12 sm-12 xs-12'>
    <h4>
      <%= inline_svg_tag('meeting_room_logo.svg', size: '52px * 36px', preserve_aspect_ratio: 'xMaxYMax meet') %>
      &nbsp;&nbsp;&nbsp;Sala Prenotabile
      <span class='pull-right'>
        <%= link_to meeting_rooms_path do %>
        <small>
          <i class='fas fa-arrow-left'></i>&nbsp;Torna alla Lista
        </small>
        <% end %>
      </span>
    </h4>
    <hr />
  </div>
</div>
<div class='row'>
  <div class='col col-lg-4 col-lg-offset-4 col-md-6 col-md-offset-3 col-sm-12 col-xs-12'>
    <dl class='dl dl-horizontal'>
      <dt>Codice:</dt>
      <dd><%= @meeting_room.code %></dd>
      <dt>Nome:</dt>
      <dd><%= @meeting_room.name %></dd>
      <dt>Descrizione</dt>
      <dd><%= @meeting_room.description %></dd>
    </dl>
  </div>
</div>